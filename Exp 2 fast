#FAST MOTION 
import cv2

cap = cv2.VideoCapture(r"E:\computer vision lab\WIN_20260212_12_40_24_Pro.mp4")

while True:
    ret, frame = cap.read()
    if not ret:
        break

    cv2.imshow("Fast Motion Video", frame)

    if cv2.waitKey(5) & 0xFF == ord('q'):
        break

cap.release()
cv2.destroyAllWindows()

